<svg class=" icon icon-outline icon-stroke-1 {{ class|default("") }}" style="fill: currentColor;" viewBox="0 0 {{ width|default('24') }} {{ height|default('24') }}" title="{{ icon }}" role="img" width="{{ width|default('24') }}px" height="{{ height|default('24') }}px">
	<use href="{{ '#' ~ icon }}" xlink:href="{{ '#' ~ icon }}"></use>
</svg>
